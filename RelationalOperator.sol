pragma solidity ^0.8.19;

//contract of relational operator
contract SolidityTest {

//declaring variable
    uint256 public a = 12;
    uint256 public b = 1;

//initializes variable with bool equal result
    bool public equal = a == b;

//initializes variable with bool not equal result
    bool public Notequal = a != b;

//initializes variable with bool greater result
    bool public greater = a > b;

//initializes variable with bool less result
    bool public less = a < b;

//initializes variable with bool greater than result
    bool public greaterEql = a >= b;

//initializes variable with bool less than result
    bool public lessEql = a <= b;


}

